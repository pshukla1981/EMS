@model EMA.Business.Factory.ViewModels.CountryModel


<div class="row">
    @using (Html.BeginForm(null, null))
    {
    <fieldset>
        <div class="col-lg-12">

            <div class="form-group">
                <label for="txtcode">Name</label>
                @Html.TextBoxFor(x => x.CountryName, new { @class = "form-control", @placeholder = "Country Name" })
                <div id="divresult"></div>
            </div>

            <div class="form-group">
                <label for="txtdepartment">Description</label>
                @Html.TextAreaFor(x => x.CountryDescription, new { @class = "form-control", @placeholder= "Description" })
            </div>

        </div>

    </fieldset>

    }


    
</div>

